{
  "workbench.editor.empty.hint": "hidden",
  "workbench.editorAssociations": {
    "{git,gitlens}:/**/*.{md,csv}": "default",
    "*.icns": "default",
    "*.exe": "default",
    "*.md": "default",
    "*.markdown": "default",
    "{git,gitlens,git-graph}:/**/*.{md,csv,svg}": "default"
  },
  "editor.fontFamily": "JetBrainsMono NF",
  "terminal.integrated.fontFamily": "JetBrainsMono NF",
  "debug.console.fontFamily": "JetBrainsMono NF",
  "editor.codeLensFontFamily": "JetBrainsMono NF",
  "editor.inlineSuggest.fontFamily": "JetBrainsMono NF",
  "editor.inlayHints.fontFamily": "JetBrainsMono NF",
  "notebook.output.fontFamily": "JetBrainsMono NF",
  "editor.fontSize": 13,
  "editor.inlineSuggest.suppressSuggestions": true,
  "editor.largeFileOptimizations": false,
  "editor.mouseWheelZoom": true,
  "editor.linkedEditing": true,
  "security.workspace.trust.untrustedFiles": "open",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "debug.onTaskErrors": "debugAnyway",
  "python.REPL.enableREPLSmartSend": false,
  "terminal.integrated.mouseWheelZoom": true,
  "explorer.compactFolders": false,
  "hediet.vscode-drawio.resizeImages": null,
  "explorer.confirmPasteNative": false,
  "color-highlight.markRuler": false,
  "redhat.telemetry.enabled": true,
  "vscode-office.editorTheme": "Auto",
  "http.proxyAuthorization": null,
  "http.proxySupport": "on",
  "vscode-office.openOutline": false,
  "vscode-office.previewCodeHighlight.showLineNumber": false,
  "editor.formatOnSave": true,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "window.zoomLevel": 0,
  "codeium.enableConfig": {
    "*": true,
    "ahk2": true,
    "ahk": true,
    "bat": true,
    "plaintext": true,
    "markdown": true,
    "properties": true,
    "twig": true
  },
  "codeium.searchMaxWorkspaceFileCount": 10000,
  "codeium.indexingRetentionPeriod": 50,
  "codeium.enableCodeLens": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.stickyTabStops": true,
  "python.createEnvironment.trigger": "off",
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.folders.associations": {
    // .vscode
    //  ╰─extensions
    //    ╰─icons
    //      ├─folder-sample.svg
    //      ╰─folder-sample-open.svg
    "xulbux": "../../../../icons/folder-xulbux"
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "eslint.format.enable": true,
  "eslint.useFlatConfig": true,
  "git.enableSmartCommit": true,
  "workbench.colorCustomizations": {},
  "docwriter.progress.trackClasses": true,
  "editor.tabSize": 4,
  "[java][json][cpp][ahk][ahk2]": {
    "editor.formatOnSave": false,
    "editor.formatOnPaste": false,
    "editor.formatOnType": false
  },
  "[vue][typescript][javascript][html]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[css][markdown]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": false
  },
  "[php]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "junstyle.php-cs-fixer"
  },
  "files.associations": {
    "*.html5": "php"
  },
  "ltex.language": "en",
  "ltex.dictionary": {
    "en": []
  },
  "php-cs-fixer.onsave": true,
  "php-cs-fixer.executablePath": "php-cs-fixer",
  "cSpell.maxNumberOfProblems": 10000,
  "cSpell.blockCheckingWhenLineLengthGreaterThan": 10000,
  "cSpell.overviewRulerColor": "#F0B7",
  "cSpell.textDecorationColor": "#F0B",
  "cSpell.textDecorationColorFlagged": "#F45",
  "cSpell.textDecorationColorSuggestion": "#8884",
  "codesnap-plus.backgroundColor": "rgba(0,0,0,0)",
  "git.openRepositoryInParentFolders": "always",
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "security.allowedUNCHosts": [
    "172.20.10.11"
  ],
  "cmake.showOptionsMovedNotification": false,
  "cmake.configureOnOpen": true,
  "cmake.generator": "Ninja",
  "cmake.buildDirectory": "${workspaceFolder}/build",
  "cmake.configureSettings": {
    "CMAKE_C_COMPILER": "C:/msys64/ucrt64/bin/gcc.exe",
    "CMAKE_CXX_COMPILER": "C:/msys64/ucrt64/bin/g++.exe"
  },
  "cmake.pinnedCommands": [
    "workbench.action.tasks.configureTaskRunner",
    "workbench.action.tasks.runTask"
  ],
  "cmake.showConfigureWithDebuggerNotification": false,
  "workbench.colorTheme": "XulbuX Neon Pro",
  "lldb.suppressUpdateNotifications": true,
  "git.ignoreRebaseWarning": true,
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#000",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#FF5055",
      "bold": true,
      "italic": false
    },
    {
      "tag": ">",
      "color": "#000",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#60FFCC",
      "bold": true,
      "italic": true
    },
    {
      "tag": "todo",
      "color": "#000",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#FF8070",
      "bold": true,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#000",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "#A090FF",
      "bold": true,
      "italic": true
    }
  ],
